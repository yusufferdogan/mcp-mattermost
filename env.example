# Mattermost Configuration (Required)
MATTERMOST_URL=https://your-mattermost-instance.com
MATTERMOST_TOKEN=your-personal-access-token

# Neo4j Configuration (Optional - for action tracking)
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password

# Cursor Integration (Optional - for user identity)
CURSOR_USER_ID=your_user_id
CURSOR_USER_NAME=Your Name
CURSOR_USER_EMAIL=your@email.com
CURSOR_USER_TEAM=Your Team

# Node Environment
NODE_ENV=production 